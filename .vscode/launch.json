{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Azure Functions & Attach Debugger",
            "type": "coreclr",
            "request": "attach",
            "preLaunchTask": "func: host start",
            "processId": "${command:azureFunctions.pickProcess}",
            "sourceFileMap": {
                "/home/site/wwwroot": "${workspaceFolder}/source"
            }
        }
    ]
}